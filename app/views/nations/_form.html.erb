
  <%= form_for [@user, @nation] do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <div class="form-group">
    <%= f.select :culture,["Steam Technology", "Magic", "Communing with Spirits"]  %>
    </div>

    <div class="form-group">
    <%= f.select :govt,["Democracy", "Monarchy", "Dictatorship", "Oligarchy", "Neighborhood Watch"]  %>
    </div>

    <div class="form-group">
    <%= f.select :econ,["Capitalism", "Socialism", "Central"]  %>
    </div>
    <div class="form-group">
    <%= f.select :geo,["Coastal", "Mountains", "Desert", "Plains", "Forest"]  %>
    </div>
    <%= f.submit %>
  <% end %>
